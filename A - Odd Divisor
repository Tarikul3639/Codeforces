#include <bits/stdc++.h>
using namespace std;
#define ll long long
#define nn '\n'
#define EPS 1e-9
#define PI 3.1415926535897932384626433832795
#define pb push_back
#define asort(a) sort(a.begin(), a.end())
#define areverse(a) reverse(a.begin(), a.end())
#define sp " "
#define test  \
    ll t;     \
    cin >> t; \
    while (t--)
int main()
{
    test
    {
        ll number;
        cin >> number;
        ll temp=number & (number-1);
        if(temp==0)
        {
            cout<<"NO"<<nn;
        }
        else{
            cout<<"YES"<<nn;
        }
    }
    return 0;
}
