#include <bits/stdc++.h>
using namespace std;
#define ll long long
#define nn '\n'
#define EPS 1e-9
#define PI 3.1415926535897932384626433832795
#define pb push_back
#define asort(a) sort(a.begin(), a.end())
#define areverse(a) reverse(a.begin(), a.end())
#define sp " "
#define test  \
    ll t;     \
    cin >> t; \
    while (t--)
int main()
{
    test
    {
        ll Tsondu,Tenzing;
        cin>>Tsondu>>Tenzing;
        ll sum1=0;
        for(ll i=0; i<Tsondu; i++)
        {
            ll x;
            cin>>x;
            sum1+=x;
        }
        ll sum2=0;
         for(ll i=0; i<Tenzing; i++)
        {
            ll x;
            cin>>x;
            sum2+=x;
        }
        ll x=sum1-sum2;
        if(x>0)
        {
            cout<<"Tsondu"<<nn;
        }
        else if(x<0)
        {
            cout<<"Tenzing"<<nn;
        }
        else if(x==0)
        {
           cout<<"Draw"<<nn;
        }
    }
    return 0;
}
